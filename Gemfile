source "https://rubygems.org"

gem "webrick"

# GitHub Pages 호환을 위해 github-pages 젬을 사용
gem "github-pages", group: :jekyll_plugins

# 특정 버전이 필요할 경우 직접 명시
gem "jekyll-sitemap", "~> 1.4.0"  # 필요시 특정 버전으로 유지
gem "jekyll-seo-tag", "~> 2.7.1"  # 필요시 특정 버전으로 유지

# 추가적인 Jekyll 플러그인을 지정할 수 있음
group :jekyll_plugins do
  gem "jekyll-feed", "~> 0.12"
end